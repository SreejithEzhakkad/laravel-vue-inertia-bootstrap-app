        "@vue/compiler-sfc": "^3.2.31",
        "autoprefixer": "^10.4.2",
        "axios": "^0.21.4",
        "bootstrap": "^5.1.3",
        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.21",
        "postcss": "^8.4.7",
        "postcss-import": "^14.0.2",
        "resolve-url-loader": "^3.1.4",
        "sass": "^1.49.9",
        "sass-loader": "^11.1.1",
        "vue": "^3.2.31",
